{
  "key": "SettingBeatport",
  "title": "Beatport User Settings by stevehero",
  "settings": [

    {
      "type": "bool",
      "key": "SettingInitialkeyAddPadding",
      "title": "Add Padding to INITIALKEY",
      "description": "8A => 08A, 7B => 07B, etc. [Default: True]",
      "default": true
    },
    {
      "type": "bool",
      "key": "SettingTotaltracksAddPadding",
      "title": "Add Padding to TOTALTRACKS",
      "description": "Adds a leading zero to TOTALTRACKS. [Default: True]",
      "default": true
    },
    {
      "type": "bool",
      "key": "SettingTrackAddPadding",
      "title": "Add Padding to TRACK",
      "description": "Adds a leading zero to TRACK. [Default: True]",
      "default": true
    },
    {
      "type": "bool",
      "key": "SettingCatIDNoAlbumAppend",
      "title": "Append CATALOG # to ALBUM / Append TRACK ID to ALBUM",
      "description": "Album Title Goes Here [MAU5CD015] [Default: True]",
      "default": true
    },
    {
      "type": "bool",
      "key": "SettingTrackTotaltracksAppend",
      "title": "Append TOTALTRACKS to TRACK",
      "description": "1/10, 2/10, 3/10 etc. [Default: True]",
      "default": true
    },
    {
      "type": "string",
      "key": "SettingDateFormat",
      "title": "DATE Format",
      "description": "The format used for the DATE field. [Default: YYYY-MM-DD]",
      "choices": [
        "YYYY-MM-DD",
        "MMDD",
        "DDMM"
      ],
      "default": "YYYY-MM-DD"
    },

    {
      "type": "bool",
      "key": "SettingFeaturingRemoveBrackets",
      "title": "feat. Artist Brackets",
      "description": "Title (feat. artist) => Title feat. artist [Default: True]",
      "default": true
    },

    {
      "type": "string",
      "key": "SettingFeaturingString",
      "title": "Featuring String",
      "description": "Choose how you want feat. string. [Default: feat.]",
